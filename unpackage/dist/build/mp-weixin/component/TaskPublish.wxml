<view data-event-opts="{{[['tap',[['jumpDetail',['$0'],['detail.task_id']]]]]}}" class="task-publish data-v-2f585153" bindtap="__e"><view class="task-head data-v-2f585153"><view class="task-head-title data-v-2f585153">{{detail.name}}</view><view class="task-tag data-v-2f585153"><block wx:if="{{detail.state==1}}"><view class="task-tag-item color-3 data-v-2f585153">审核中</view></block><block wx:if="{{detail.state==2}}"><view class="task-tag-item color-2 data-v-2f585153">已通过</view></block><block wx:if="{{detail.state==3}}"><view class="task-tag-item color-5 data-v-2f585153">未通过</view></block><block wx:if="{{detail.state==4}}"><view class="task-tag-item color-1 data-v-2f585153">已上架</view></block><block wx:if="{{detail.state==5}}"><view class="task-tag-item color-4 data-v-2f585153">已结束</view></block></view></view><view class="task-price data-v-2f585153"><view class="price data-v-2f585153">{{"￥"+detail.moeny}}</view><view class="vip-price data-v-2f585153">{{"VIP:￥"+detail.vip_moeny}}</view></view><view class="task-content data-v-2f585153"><text class="data-v-2f585153">{{detail.desc}}</text></view><view class="task-footer data-v-2f585153"><view class="task-footer-timer data-v-2f585153"><text class="data-v-2f585153">{{"截止时间："+detail.end_time}}</text></view><block wx:if="{{detail.state==4||detail.state==5}}"><view class="task-footer-num data-v-2f585153"><text class="active data-v-2f585153">{{detail.member_accept_task+"人参加"}}</text></view></block></view></view>