<view class="container"><block wx:if="{{params.state==3&&isFail}}"><view class="top-tips"><view class="top-tips-text"><view class="top-tips-line">{{params.desc}}</view></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="top-tips-close" bindtap="__e"><u-icon vue-id="93392e42-1" name="close" size="40" color="#ED4015" bind:__l="__l"></u-icon></view></view></block><view class="app-box"><view class="app-box-item"><image class="icon" src="/static/icon/dian.png"></image><text class="title">真实店名</text><view class="app-box-cont"><input placeholder-class="placeholder" type="text" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['params']]]]]}}" value="{{params.name}}" bindinput="__e"/></view></view><view class="app-box-item"><image class="icon" src="/static/icon/dui.png"></image><text class="title">身份证号</text><view class="app-box-cont"><input maxlength="18" placeholder-class="placeholder" type="idcard" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['$0','numbers_id','$event',[]],['params']]]]]}}" value="{{params.numbers_id}}" bindinput="__e"/></view></view><view class="app-box-item"><image class="icon" src="/static/icon/phone.png"></image><text class="title">手机号码</text><view class="app-box-cont"><input maxlength="11" placeholder-class="placeholder" type="number" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['params']]]]]}}" value="{{params.phone}}" bindinput="__e"/></view></view><view class="app-box-item"><image class="icon" src="/static/icon/dianhua.png"></image><text class="title">店铺电话</text><view class="app-box-cont"><input maxlength="12" placeholder-class="placeholder" type="number" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['$0','dian_phone','$event',[]],['params']]]]]}}" value="{{params.dian_phone}}" bindinput="__e"/></view></view><view class="app-box-item"><image class="icon" src="/static/icon/didian.png"></image><text class="title">所在区域</text><view data-event-opts="{{[['tap',[['areaChange',['$event']]]]]}}" class="app-box-cont" bindtap="__e"><block wx:if="{{params.address===''}}"><text class="placeholder">请选择</text></block><block wx:else><text class="active">{{params.address}}</text></block><u-icon vue-id="93392e42-2" name="arrow-right" size="28" color="#333" bind:__l="__l"></u-icon></view></view></view><view class="app-context"><view class="app-title"><view class="Title-line"></view><view class="Title-text">门店LOGO</view></view><view class="app-context-box"><view data-event-opts="{{[['tap',[['uploadLogo',['$event']]]]]}}" class="app-context-add" bindtap="__e"><block wx:if="{{params.logo===''}}"><image src="/static/image/add.png"></image></block><block wx:else><u-image vue-id="93392e42-3" width="100%" height="100%" src="{{logoImg}}" bind:__l="__l"></u-image></block></view></view></view><view class="app-context"><view class="app-title"><view class="Title-line"></view><view class="Title-text">其他资料</view></view><view class="app-image"><block wx:for="{{photoImg}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="app-image-item"><u-image vue-id="{{'93392e42-4-'+index}}" src="{{item.abs_url}}" width="336rpx" height="200rpx" bind:__l="__l"></u-image></view></block><view data-event-opts="{{[['tap',[['uploadImage',['$event']]]]]}}" class="app-image-item" bindtap="__e"><image class="icon" src="/static/image/qita.png"></image></view></view></view><block wx:if="{{params.state==0}}"><view data-event-opts="{{[['tap',[['taskParams',['$event']]]]]}}" class="defalut-btn" bindtap="__e">提交</view></block><block wx:if="{{params.state==1}}"><view class="defalut-btn">正在审核,请耐心等待...</view></block><block wx:if="{{params.state==3}}"><view data-event-opts="{{[['tap',[['taskParams',['$event']]]]]}}" class="defalut-btn" bindtap="__e">重新提交</view></block></view>